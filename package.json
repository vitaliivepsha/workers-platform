  "version": "1.0.0",
  "description": "Use it for simple and fast web development",
  "main": "index.js",
  "scripts": {
    "dev": "SET \"NODE_ENV=development\" && webpack --config vendor.webpack.config.js && webpack-dev-server --progress --port 88",
    "build": "rd /s /q dist && SET \"NODE_ENV=production\" && webpack -p --colors",
    "vendor": "SET \"NODE_ENV=production\" && webpack --config vendor.webpack.config.js",
    "build-test": "node .bin/build-test",
    "test": "SET \"NODE_ENV=tdd\" && npm run build-test",
    "test-ci": "SET \"NODE_ENV=test-ci\" && testem ci"
